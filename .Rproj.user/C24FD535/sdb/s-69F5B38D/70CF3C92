ary(rootsE.E.lm)\nsummary(rootsE.A.lm)\nAnova(rootsE.A.lm, rootsE.E.lm)\n\n\nrootsE0.lm <- lm(dryBiomass ~ WUE, data = traits.E__)\nrootsE1.lm <- lm(dryBiomass ~ WUE * treatment, data = traits.E__)\nrootsE2.lm <- lm(dryBiomass ~ WUE * CO2, data = traits.E__)\nrootsE3.lm <- lm(dryBiomass ~ WUE * treatment * CO2, data = traits.E__)\nrequire(MuMIn)\n\nAICc(rootsE0.lm, rootsE1.lm, rootsE2.lm, rootsE3.lm) # significant treatment interaction\n\nanova(rootsE0.lm)\nanova(rootsE3.lm)\n\n\n\n\n### ACACIA ###\n\ntraits.A__ <- traits.A\n#traits.A__ <- subset(traits.A, dryBiomass < 0.3)\n#traits.A__ <- traits.A__[-25,]\n                     \nplot(traits.A__$WUE, traits.A__$dryBiomass, col = traits.A__$treatment, pch = as.numeric(traits.A__$CO2))\nrootsA.control.lm <- lm(dryBiomass ~ WUE, data = subset(traits.A__, treatment == \"control\"))\nrootsA.recovery.lm <- lm(dryBiomass ~ WUE, data = subset(traits.A__, treatment == \"recovery\"))\nrootsA.flooded.lm <- lm(dryBiomass ~ WUE, data = subset(traits.A__, treatment == \"flooded\"))\n\nabline(rootsA.control.lm, col = c(\"black\"))\nabline(rootsA.recovery.lm, col = c(\"green\"))\nabline(rootsA.flooded.lm, col = c(\"red\"))\nsummary(rootsA.control.lm)\nsummary(rootsA.recovery.lm)\nsummary(rootsA.flooded.lm)\n\nrootsA.E.lm <- lm(dryBiomass ~ WUE, data = subset(traits.A__, CO2 == \"E\"))\nrootsA.A.lm <- lm(dryBiomass ~ WUE, data = subset(traits.A__, CO2 == \"A\"))\n\nplot(traits.A__$WUE, traits.A__$dryBiomass, col = traits.A__$CO2, pch = as.numeric(traits.A__$CO2))\n\nabline(rootsA.E.lm, col = c(\"red\"))\nabline(rootsA.A.lm, col = c(\"black\"))\nsummary(rootsA.E.lm)\nsummary(rootsA.A.lm)\n\n\nrootsA0.lm <- lm(dryBiomass ~ WUE, data = traits.A__)\nrootsA1.lm <- lm(dryBiomass ~ WUE * treatment, data = traits.A__)\nrootsA2.lm <- lm(dryBiomass ~ WUE * CO2, data = traits.A__)\nrootsA3.lm <- lm(dryBiomass ~ WUE * treatment * CO2, data = traits.A__)\nrequire(MuMIn)\n\nAICc(rootsA0.lm, rootsA1.lm, rootsA2.lm, rootsA3.lm) # rootsA2 wins!\n\nanova(rootsA3.lm) #  significant treatment and highly significant CO2 interactions\nanova(rootsA2.lm) # \nanova(rootsA1.lm) #\n\n## CASUARINA ##\n\ntraits.C__ <- traits.C\n#traits.C__ <- subset(traits.C, dryBiomass < 0.31)\n\nplot(traits.C__$WUE, traits.C__$dryBiomass, col = traits.C__$treatment, pch = as.numeric(traits.C__$CO2))\nrootsC.control.lm <- lm(dryBiomass ~ WUE, data = subset(traits.C__, treatment == \"control\"))\nrootsC.recovery.lm <- lm(dryBiomass ~ WUE, data = subset(traits.C__, treatment == \"recovery\"))\nrootsC.flooded.lm <- lm(dryBiomass ~ WUE, data = subset(traits.C__, treatment == \"flooded\"))\n\nabline(rootsC.control.lm, col = c(\"black\"))\nabline(rootsC.recovery.lm, col = c(\"green\"))\nabline(rootsC.flooded.lm, col = c(\"red\"))\nsummary(rootsC.control.lm)\nsummary(rootsC.recovery.lm)\nsummary(rootsC.flooded.lm)\n\n\nrootsC.E.lm <- lm(dryBiomass ~ WUE, data = subset(traits.C__, CO2 == \"E\"))\nrootsC.A.lm <- lm(dryBiomass ~ WUE, data = subset(traits.C__, CO2 == \"A\"))\n\nplot(traits.C__$WUE, traits.C__$dryBiomass, col = traits.C__$CO2, pch = as.numeric(traits.C__$CO2))\n\nabline(rootsC.E.lm, col = c(\"red\"))\nabline(rootsC.A.lm, col = c(\"black\"))\nsummary(rootsC.E.lm)\nsummary(rootsC.A.lm)\n\n\nrootsC0.lm <- lm(dryBiomass ~ WUE, data = traits.C__)\nrootsC1.lm <- lm(dryBiomass ~ WUE * treatment, data = traits.C__)\nrootsC2.lm <- lm(dryBiomass ~ WUE * CO2, data = traits.C__)\nrootsC3.lm <- lm(dryBiomass ~ WUE * treatment * CO2, data = traits.C__)\nrequire(MuMIn)\n\nAICc(rootsC0.lm, rootsC1.lm, rootsC2.lm, rootsC3.lm)\n\nanova(rootsC1.lm)\nanova(rootsC2.lm) \nanova(rootsC3.lm) # significant treatment and Co2 effects, and significant treatment interaction, \n                  # almost significant three-way interaction!\n\n\n\n",
